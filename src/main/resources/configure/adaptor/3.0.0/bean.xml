<?xml version="1.0" encoding="euc-kr"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
		<bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer" id="propertyConfigurer">
        <property name="locations">
            <list>
                <value>classpath:@@IN_NAME@@.properties</value>
            </list>
        </property>
    </bean>

     <bean id="adaptorBean" class="com.mb.indigo2.springsupport.AdaptorConfigBean">
      <property name="adaptorName" value="@@IN_NAME@@"/>
        <property name="port" value="@@PORT@@"></property>
        <property name="heartBeatInterval" value="60"></property>
        <property name="socketTimeOut" value="60"></property>
        <property name="heartbeatMsgType" value="xml"></property>
        <property name="netChkList">
            <list>
            </list>
        </property>
    </bean>
 </beans>