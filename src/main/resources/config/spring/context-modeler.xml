<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">
	
	<bean id="componentConfigParserManager" class="com.indigo.web.visualization.modeler.parser.ComponentConfigureRepository">
		<property name="propertyPathList">
			<list>
				<value>config/modeler/endpointConfigure.xml</value>
				<value>config/modeler/routeConfigure.xml</value>
				<value>config/modeler/controlflowConfigure.xml</value>
				<value>config/modeler/transformationConfigure.xml</value>
				<!-- <value>config/modeler/etcConfigure.xml</value> -->
			</list>
		</property>
	</bean>
</beans>	
