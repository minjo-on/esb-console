	</component>
	
	<component type="process" alias="Processor" icon="Process.png" tagName="process">
		<attr name="ref" alias="Processor Ref" inputType="ref" option="custombean"/>
	</component>
	
	<component type="netty" alias="TCP" icon="CXF-RS.png">
		<uri>
			<host name="uri" alias="URI" default="tcp://"/>
			<query name="encoding" alias="Encoding" default="EUC-KR" />
			<query name="sync" alias="Sync" default="true" inputType="boolean"/>
			<query name="textline" alias="Textline" default="true" inputType="boolean"/>
		</uri>
	</component>
	<component type="velocity" alias="TemplateXml" icon="Mapping.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.VelocityEndpointParser" isUse="false">
		<uri>
			<component name="component" inputType="hidden" default="velocity"/>
			<host name="filename" alias="filename" required="true" inputType="extEditor" option="IndigoModeler.view.center.property.VelocityEditor" />
		</uri>
	</component>
	<component type="cxf" alias="WebService" icon="CXF-RS.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.WebServiceEndpointParser">
		<uri>
			<host name="prefix" default="bean" inputType="hidden"/>
			<query name="dataFormat" alias="Data Format" default="PAYLOAD" inputType="select" option="['POJO','PAYLOAD','MESSAGE']"/>
			<query name="useType" default="true"  inputType="hidden" />
		</uri>
		<!-- bean start -->
		<bean>
			<!-- <prop name="cxfId" alias="아이디" default="" required="true" /> -->
			<!-- <prop name="cxfWsdl" alias="WSDL" inputType="extEditor" option="IndigoModeler.view.center.property.PropWebServiceWSDLEditor" required="true" /> -->
			<prop name="cxf_wsdlURL" alias="WSDL" inputType="extEditor" option="IndigoModeler.view.center.property.PropWebServiceWSDLEditor" required="true" />
			<prop name="cxf_namespace" alias="네임스페이스" default="" required="true" />
			<prop name="cxf_serviceName" alias="서비스 이름" default="" required="true" />
			<prop name="cxf_endpointName" alias="포트 이름" inputType="extEditor" option="IndigoModeler.view.center.property.PropCxfPortnameEditor" required="true" />
			<prop name="soap_version" alias="SOAP 버전" inputType="select" default="1.1" option="['1.1', '1.2']" required="true" />
			<!-- <prop name="cxfSOAPHeaderUseYn" alias="SOAP 헤더 사용" inputType="boolean" default="false" required="true" />
			<prop name="cxfMessageLogUseYn" alias="메시지 로그 사용" inputType="boolean" default="false" required="true" /> -->
			<prop name="cxf_address" alias="서비스 주소" default="" required="true" />
		</bean>
		<!-- bean end -->
	</component>
	<component type="xslt" alias="Xslt" icon="XSLT.png">
		<uri>
			<host name="xsltFile" alias="Xslt File" inputType="extEditor" option="IndigoModeler.view.center.property.XsltEditor" required="true" />
		</uri>
	</component>
</componentConfigure>