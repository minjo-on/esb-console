<?xml version="1.0" encoding="UTF-8"?>
<!-- 컴포넌트 등록 시 알파벳 순으로 등록 -->
<componentConfigure type="endpoint">
	<component type="indigoAssert" alias="AssertEquals" icon="AssertEquals.png" isUse="false">
		<uri>
			<host name="equals" inputType="hidden" default="equals"/>
			<query name="checkKey" alias="Compare Data" inputType="extEditor" option="IndigoModeler.view.center.property.MessageTypeSelectEditor" required="true"/>
			<query name="checkValue" alias="Compare Value" required="true"/>
			<query name="errorKey" alias="Error Data" inputType="extEditor" option="IndigoModeler.view.center.property.MessageTypeSelectEditor" />
			<query name="errorMessage" alias="Default Error Message"/>
		</uri>
	</component>
	<component type="bean" alias="Bean" icon="MessageEndpoint.png" isUse="false">
		<uri>
			<host name="ref" alias="Bean Ref" inputType="ref" option="custombean" required="true">
				<description>
					사용자 개발 클래스 사용
					Bean Configuration CustomBean를 등록해 주십시오.
				</description>
			</host>
			<query name="method" alias="Method" validationExpression="" required="true">
				<description>
					CustomBean 클래스의 메소드 명을 입력해 주십시오.
				</description>
			</query>
		</uri>
	</component>
	<component type="internal" alias="BridgePoint" icon="Security.png">
		<uri>
			<component name="component" alias="Protocol" default="direct" inputType="protocol" option="IndigoModeler.view.center.property.BridgePointProtocolEditor" required="true"/>
			<host name="uri" alias="Endpoint URI" inputType="bridge" option="IndigoModeler.view.center.property.BridgePointEndpointURIEditor" required="true"/>
		</uri>
	</component>
	<component type="indigoCompress" alias="Compress" icon="Process.png" isUse="false">
		<uri>
			<host name="operation" alias="Operation" inputType="select" option="['compress','decompress']" required="true"/>
			<query name="zipDelete" alias="Temp File Delete" inputType="boolean" default="true"/>
			<query name="compressor" alias="Custom Compressor" inputType="ref" option="custombean"/>
		</uri>
	</component>
	<!-- SQL Excutor로 변경 -->
	<component isUse="true" type="indigoDBReader" alias="DBReader" icon="dbcp_1.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.IndigoDBReaderEndpointParser">
		<uri>
			<component name="component" inputType="hidden" default="indigoDB"/>
			<host name="dataSource" alias="Data Source" inputType="ref" option="dbcp" required="true"/>
			<query name="selectType" alias="Select Type" inputType="select" default="Table" option="['Table', 'Procedure']"/>
			<query name="ifId" alias="Interface Id" required="true"/>
			<query name="delay" alias="Timer Interval(ms)" inputType="number" default="3000"/>
			<query name="query" alias="Select Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropMybatisQueryEditor" required="true"/>
			<query name="type" alias="Message Type" inputType="select" default="Object" option="['Obejct', 'XML', 'File']"/>
			<query name="maxCount" alias="Select Row Count" inputType="number" default="-1"/>
			<query name="useUpdate" alias="Use Status Update" inputType="select" default="false" option="['true','false']"/>
			<query name="doneQuery" alias="Status Update Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropMybatisQueryEditor"/>
			<query name="isMulti" alias="Parallel Processing" inputType="select" default="true" option="['true','false']"/>
			<query name="threadCount" alias="Thread Count" inputType="number" default="5" compare="data['isMulti'] =='true'"/>
			<query name="maxRowCount" alias="Message Count Per Thread" inputType="number" default="3000" compare="data['isMulti']=='true'"/>
		</uri>
	</component>
	<component isUse="false" type="indigoDBWriter" alias="DBWriter" icon="dbcp_1.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.IndigoDBWriterEndpointParser">
		<uri>
			<component name="component" inputType="hidden" default="indigoDB"/>
			<host name="dataSource" alias="Data Source" inputType="ref" option="dbcp" required="true"/>
			<query name="query" alias="Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropSQLBuilderInsertEditor"/>
		</uri>
	</component>
	<component type="fileReader" alias="FileReader" icon="FileMove.png">
		<uri>
			<host name="filePath" alias="FilePath" default="" required="true"/>
			<!-- <query name="charset" alias="Charset"/> -->
		</uri>
	</component>
	<component type="file" alias="FileWriter" icon="FileMove.png">
		<uri>
			<host name="filePath" alias="FilePath" default="" required="true"/>
			<!-- <query name="fileMove" alias="File Move" default="false" inputType="boolean"/> -->
			<query name="charset" alias="Charset" default="UTF-8"/>
			<query name="delay" alias="Delay" default="10000"/>
		</uri>
	</component>
	<component type="ftpreader" alias="FTPReader" icon="FTP.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.FTPEndpointParser">
		<uri>
			<host name="uri" alias="URI" default="ftp://" required="true"/>
			<query name="username" alias="User" required="true"/>
			<query name="password" alias="Password" required="true"/>
			<query name="binary" alias="Binary" default="true" inputType="boolean"/>
			<query name="localWorkDirectory" alias="Local Directory" default="./temp"/>
			<query name="move" alias="Move path" default=".done"/>
			<query name="moveFailed" alias="Move Failed path"/>
			<query name="listFile" alise="List File" default="false" inputType="boolean"/>
			<!--  <query name="delay" alias="Delay" default="10000" inputType="number"/> -->
		</uri>
	</component>
	<component type="ftpwriter" alias="FTPWriter" icon="FTP.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.FTPEndpointParser">
		<uri>
			<host name="uri" alias="URI" default="ftp://" required="true"/>
			<query name="username" alias="User" required="true"/>
			<query name="password" alias="Password" required="true"/>
			<query name="binary" alias="Binary" default="true" inputType="boolean"/>
		</uri>
	</component>
	<component type="jetty" alias="HTTP" icon="CXF-RS.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.HttpEndpointParser">
		<uri>
			<host name="uri" alias="URI" default="http://0.0.0.0" required="true"/>
			<query name="httpClient.timeout" alias="Timeout" default="30000" inputType="number"/>
			<query name="matchOnUriPrefix" alias="Match URI Prefix" default="false" inputType="boolean"/>
			<query name="throwExceptionOnFailure" alias="Throw Exception Failure" default="false" inputType="boolean"/>
		</uri>
	</component>
	<!-- <component type="jca" alias="JCA" icon="MessageEndpoint.png">
		<uri>
			<host name="jca" alias="JCA" />
		</uri>
	</component> -->
	<component type="log" alias="Log" icon="Log.png" isUse="true" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.LogEndpointParser" >
		<uri>
			<query name="message" alias="Message" default="${body}"/>
			<query name="loggingLevel" alias="Level" default="INFO" inputType="select" option="['DEBUG','INFO','WRAN','ERROR']"/>
		</uri>
	</component>
	<component type="indigoTransformer" alias="MappingVariable" icon="Mapping.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.IndigoTransformerEndpointParser" isUse="false">
		<uri>
			<component name="component" inputType="hidden" default="indigoTransformer"/>
			<host name="transformerId" alias="Transformer Id" inputType="extEditor" option="IndigoModeler.view.center.property.TransformerEditor" required="true" />
		</uri>
	</component>
	<component type="pop3" alias="Mail" icon="SMTP.png">
		<uri>
			<host name="pop3" alias="URI" default="//" required="true"/>
			<query name="username" alias="User" required="true"/>
			<query name="password" alias="Password" required="true"/>
			<query name="delete" alias="delete" default="true"/>
			<query name="unseen" alias="unseen" default="true"/>
			<query name="consumer.delay" alias="consumer.delay" default="10000"/>
			<query name="contentType" alias="contentType" default="text/xml"/>
		</uri>
	</component>
	<component type="indigoTransfer" alias="MessageQueue" icon="JMS.png">
		<uri>
			<host name="ip" alias="IP" required="true" default="localhost"/>
			<host name="port" alias="Port" required="true" default="24211"/>
			<host name="queueName" alias="Queue Name" required="true" default="ROUTE.IN"/>
			<query name="consumerCount" alias="Consumer Count" inputType="number" default="10"/>
			<query name="resultQueueName" alias="Result Queue Name" required="false" default="RETURN.IN"/>
			<!-- <query name="destId" alias="Dest ID" required="false"/> -->
		</uri>
	</component>
	<component type="mapConvertor" alias="MapConvertor" icon="Mapping.png" isUse="false">
		<uri>
			<host name="processor" inputType="hidden"  default="map"/>
			<query name="charset" alias="Charset" default="EUC-KR"/>
			<query name="messageType" alias="Message Type" inputType="select" option="['XML','DELIMITER']" default="XML"/>
			<query name="rowPath" alias="Row xpath" default="/root/sndbizdata/rows" compare="data['messageType']=='XML')"/>
			<query name="delimiter" alias="Delimiter" default="," compare="data['messageType']=='DELIMITER'"/>
		</uri>
	</component>
	<component type="indigoProcessor" alias="Processor" icon="Process.png">
		<uri>
			<host name="ref" alias="Processor Ref" inputType="ref" option="custombean" required="true"/>
			<!-- <query name="method" alias="Method" validationExpression="" required="true">
				<description>
					CustomBean 클래스의 메소드 명을 입력해 주십시오.
				</description>
			</query> -->
		</uri>
	</component>
	<component type="quartz2" alias="Scheduler" icon="Quartz2.png" isUse="true" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.Quartz2EndpointParser">
		<uri>
			<!-- <query name="serviceName" alias="Service Name" required="true" /> -->
			<query name="triggerType" alias="Type" inputType="select" option="['Simple', 'Cron']" default="Cron" required="true" />
			<query name="repeatCount" alias="Repeat Count" compare="data['triggerType']=='Simple'" default="-1" inputType="number" required="true" />
			<query name="repeatInterval" alias="Repeat Interval" compare="data['triggerType']=='Simple'" default="1000" inputType="number" required="true" />
			<query name="cron" alias="Cron" compare="data['triggerType']=='Cron'" inputType="extEditor" option="IndigoModeler.view.center.property.Quartz2CronEditor" required="true" />
		</uri>
		<bean>
			<prop name="org.quartz.threadPool.threadCount" alias="Thread count" default="1"/>
		</bean>
	</component>
	<component type="cxfrs" alias="Rest" icon="CXF-RS.png" isUse="false">
		<uri>
			<host name="prefix" default="bean" inputType="hidden"/>
			<host name="cxfref" alias="Rest Ref" inputType="ref" option="cxfrs" required="true" />
		</uri>
	</component>
	<component type="restlet" alias="Rest" icon="CXF-RS.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.RestletEndpointParser">
		<uri>
			<host name="restletUri" default="http://0.0.0.0" alias="Restlet Uri" inputType="extEditor" option="IndigoModeler.view.center.property.PropRestletEditor" required="true" />
			<query name="restletMethod" alias="Restlet Method" inputType="select" option="['get','put','post','patch','delete','head','trace','connect']" default="get" />
			<query name="restletRealm" alias="Restlet Realm" inputType="ref" option="custombean" />
		</uri>
	</component>
