	<component type="sapfn" alias="SAP" icon="SAP.png">
		<uri>
			<host name="connectionFactory" alias="SAP Connection Ref" inputType="ref" option="sapfn" required="true">
				<description>
					SAP 연결정보를 등록해 주십시오.
				</description>
			</host>
			<!-- <query name="destinationName" alias="Destination Name" required="true"/> -->
			<query name="functionName" alias="Function Name" required="true"/>
			<!-- <query name="inParamStrucName" alias="InParamStrucName" required="true"/>
			<query name="outParamStrucName" alias="OutParamStrucName" required="true"/>
			<query name="inTableName" alias="InTableName" required="true"/>
			<query name="outTableName" alias="OutTableName" required="true"/> -->
		</uri>
		
	</component>
	
	<component type="ssh" alias="SSH" icon="Process.png">
		<uri>
			<host name="host" alias="HOST" default="localhost:22" required="true"/>
			<query name="username" alias="User Name" required="true"/>
			<query name="password" alias="Password" required="true"/>
		</uri>
	</component>
	
	<component type="indigoSecurity" alias="Security" icon="Security.png" isUse="false">
		<uri>
			<host name="operation" alias="Operation" inputType="select" option="['encrypt','decrypt']" required="true"/>
			<query name="securityKey" alias="Security Key" default="KEY"/>
			<query name="securitor" alias="Custom Securitor" inputType="ref" option="custombean"/>
		</uri>
	</component>
	<component type="indigoSch" alias="Scheduler" icon="Quartz2.png" isUse="false">
		<uri>
			<host name="timer" alias="Scheduler Ref" inputType="ref" option="indigosch" required="true" />
		</uri>
	</component>
	<component type="timer" alias="SimpleTimer" icon="Quartz2.png" isUse="false">
		<uri>
			<host name="timer" alias="Timer Name"/>
			<query name="period" alias="Period(ms)" default="1000" inputType="number"/>
		</uri>
	</component>
	<component type="iBatchReader" alias="DBBatchReader" icon="SQL.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.RefIDAutoParser" isUse="false">
		<!-- bean start -->
		<bean>
			<prop name="iBatchDs" alias="DataSource" inputType="extEditor" option="IndigoModeler.view.center.property.PropIBatchDataSourceEditor" required="true" />
			<prop name="iBatchType" alias="Type" default="SELECT" required="true" />
			<prop name="iBatchQuery" alias="Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropIBatchQueryEditor" required="true" />
			<!-- reader option -->
			<prop name="iBatchRealTimeCount" alias="RealTimeCount" default="true" inputType="boolean" required="true" />
			<prop name="iBatchTotalRowCount" alias="TotalRowCount" default="100000" inputType="number" required="true" />
			<prop name="iBatchMaxRowCount" alias="MaxRowCount" default="100000" inputType="number" required="true" />
		</bean>
	</component>
	<component type="iBatchWriter" alias="DBBatchWriter" icon="SQL.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.RefIDAutoParser" isUse="false">
		<!-- bean start -->
		<bean>
			<prop name="iBatchDs" alias="DataSource" inputType="extEditor" option="IndigoModeler.view.center.property.PropIBatchDataSourceEditor" required="true" />
			<prop name="iBatchType" alias="Type" default="INSERT" required="true" />
			<prop name="iBatchQuery" alias="Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropIBatchQueryEditor" required="true" />
			<!-- writer option -->
			<prop name="iBatchBatchCount" alias="BatchCount" default="1000" inputType="number" required="true" />
			<prop name="iBatchCommitCount" alias="CommitCount" default="5000" inputType="number" required="true" />
			<prop name="iBatchMaxQueueCount" alias="MaxQueueCount" default="1000" inputType="number" required="true" />
			<prop name="iBatchThreadCount" alias="ThreadCount" default="1" inputType="number" required="true" />
			<prop name="iBatchMaxPollTime" alias="MaxPollTime" default="1000" inputType="number" required="true" />
			<prop name="iBatchIsMulti" alias="IsMulti" default="false" inputType="boolean" required="true" />
		</bean>
	</component>
	<component type="mybatis" alias="SQLExecutor" icon="SQL.png" parserClass="com.indigo.web.visualization.modeler.parser.component.endpoint.MybatisEndpointParser">
		<!-- bean start -->
		<bean>
			<prop name="mybatisDs" alias="DataSource" inputType="extEditor" option="IndigoModeler.view.center.property.PropMybatisDataSourceEditor" required="true" />
			<!-- <prop name="mybatisType" alias="Type" default="SELECT" inputType="extEditor" option="IndigoModeler.view.center.property.PropMybatisQueryTypeEditor" required="true" />
			<prop name="mybatisQuery" alias="Query" inputType="extEditor" option="IndigoModeler.view.center.property.PropMybatisQueryEditor" required="true" /> -->
		</bean>
		<uri>
			<query name="mybatisType" 	alias="Type" 				 inputType="extEditor" 	default="SELECT" option="IndigoModeler.view.center.property.PropMybatisQueryTypeEditor" required="true" />
			<query name="mybatisQuery" 	alias="Query" 				 inputType="extEditor" 	option="IndigoModeler.view.center.property.PropMybatisQueryEditor" required="true" />
			<query name="useFile" 		alias="Use File" 			 inputType="boolean" 	default="false" compare="data['mybatisType']=='SELECT'"/>
			<query name="executorType" 	alias="Executor Type" 		 inputType="select" 	default="SIMPLE" option="['SIMPLE','BATCH']" compare="data['mybatisType']!='SELECT'"/>
			<query name="useOnlyHeader" alias="Use OnlyHeader" 		 inputType="boolean" 	default="false"/>
			<query name="noDataRouteStop" alias="No Data Route Stop" inputType="boolean" 	default="true" compare="data['mybatisType']=='SELECT'"/>
			<query name="batchCount"	alias="Batch Count" 		 inputType="number" 	default="100000" compare="data['mybatisType']!='SELECT' &amp;&amp; data['executorType']=='BATCH'"/>
			<query name="useCached" alias="Use Cached" inputType="select" 	default="true" option="['true','false']" compare="data['mybatisType']=='SELECT'"/>
			<query name="maxSize" alias="Max Size" inputType="number" 	default="5000" compare="data['useCached'] =='true'"/>
		</uri>
		<!-- bean end -->
